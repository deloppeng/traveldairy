<h1>picsys - pic</h1>
    <table>
        <tr>
            <th>id</th>
            <th>title</th>
            <th>comment</th>
            <th>path</th>
        </tr>
        <% for ( var i = 0 ; i < data.length ; i++){ %>
            <tr>
                <td>
                    <%= data[i].id  %>
                </td>
                <td>
                    <%= data[i].title  %>
                </td>
                <td>
                    <%= data[i].comment  %>
                </td>
                <td>
                    <%= data[i].path  %>
                </td>
            </tr>
            <% } %>
    </table>
